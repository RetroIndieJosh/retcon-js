{
        "compilerOptions": {
                "esModuleInterop": true,
                "forceConsistentCasingInFileNames": true,
                "module": "commonjs",
                "noImplicitAny": true,
                "outDir": "js",
                "preserveConstEnums": true,
                "removeComments": true,
                "skipLibCheck": true,
                "strict": true,
                "target": "ES2015"
        },
        "files": [
                "main.ts",
                "video.ts"
        ],
        "$schema": "https://json.schemastore.org/tsconfig",
        "display": "Recommended"
}
